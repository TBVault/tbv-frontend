    "/public/hello_world_pydantic": {
      "get": {
        "summary": "Public Hello World Pydantic",
        "operationId": "public_hello_world_pydantic_public_hello_world_pydantic_get",
        "parameters": [
          {
            "name": "message",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Message"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HelloDto"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/transcript": {
      "get": {
        "summary": "Transcript",
        "operationId": "transcript_transcript_get",
        "parameters": [
          {
            "name": "public_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
