              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/protected/helloworld": {
      "get": {
        "summary": "Protected Hello World",
        "description": "Protected hello world endpoint - requires valid JWT token.\nReturns \"helloworld\" string.",
        "operationId": "protected_hello_world_protected_helloworld_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HelloDto"
                }
              }
            }
          }
        },
        "security": [
          {
            "OpenIdConnect": []
          },
          {
            "HTTPBearer": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "HelloDto": {
        "properties": {
          "message": {
            "type": "string",
            "title": "Message"
          }
        },
        "type": "object",
        "required": [
          "message"
        ],
        "title": "HelloDto"
      },
      "Transcript": {
        "properties": {
          "public_id": {
            "type": "string",
            "title": "Public Id"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "duration": {
            "type": "integer",
            "title": "Duration"
          },
          "summary": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Summary"
          },
          "content": {
            "items": {
              "$ref": "#/components/schemas/TranscriptChunk"
            },
            "type": "array",
            "title": "Content"
          }
        },
        "type": "object",
        "required": [
          "public_id",
          "title",
          "duration",
          "summary",
          "content"
        ],
        "title": "Transcript"
      },
      "TranscriptChunk": {
        "properties": {
          "speaker": {
            "type": "string",
            "title": "Speaker"
          },
          "start": {
            "type": "integer",
            "title": "Start"
          },
          "text": {
            "type": "string",
            "title": "Text"
          }
        },
        "type": "object",
        "required": [
          "speaker",
          "start",
          "text"
        ],
        "title": "TranscriptChunk"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    },
    "securitySchemes": {
      "OpenIdConnect": {
        "type": "openIdConnect",
        "openIdConnectUrl": "https://accounts.google.com/.well-known/openid-configuration"
      },
      "HTTPBearer": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}